{
  "name": "flutter",
  "version": "1.0.0",
  "description": "A simple rule-validation API",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node ./src",
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && babel ./src -d ./dist",
    "start": "npm run build && node ./dist/",
    "test": "cross-env NODE_ENV=test nyc mocha --timeout 20000 --exit",
    "cover": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls": "coveralls < lcov.info",
    "coverage": "npm run cover && npm run coveralls"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "mocha": "^8.2.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.5.0"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "engines": {
    "node": "14.15.1",
    "npm": "6.14.8"
  }
}
